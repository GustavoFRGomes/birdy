# This is the layout file of the search_form

SearchForm:
	orientation: 'vertical'

	BoxLayout:
		orientation:'horizontal'
		size_hint_y: 0.2

		TextInput:
			multiline: False
			id: search_keyword
			size_hint_x: 0.7
		Button:
			text: 'Search'
			size_hint_x: 0.15
		Button:
			text: 'Cancel'
			size_hint_x: 0.15

	BoxLayout:
		size_hint_y: 0.8
